      "name": "TEA Index"
    },
    {
      "src": "src/bmm/testarch/knowledge",
      "dest": "bmm/tea/knowledge",
      "destFlat": "bmm-tea/knowledge",
      "name": "TEA Knowledge Base",
      "isDirectory": true
    },
    {
      "src": "src/core/resources/excalidraw",
